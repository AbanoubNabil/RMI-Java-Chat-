<AnchorPane id="AnchorPane" prefHeight="605.0" prefWidth="534.0" style="-fx-background-color: #fccc66;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignUpControllerFromAdmin">
    <stylesheets>
        <URL value="@signup.css" />
