        </RadioButton>
        <Label layoutX="79.0" layoutY="447.0" prefHeight="42.0" prefWidth="59.0" text="Gender" textFill="#807474">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </Label>
        <Label layoutX="55.0" layoutY="373.0" />
        <ComboBox fx:id="country" layoutX="260.0" layoutY="503.0" onMouseDragged="#drag2" onMousePressed="#drag1" prefHeight="25.0" prefWidth="180.0" promptText="Egypt" />
           
        <Label layoutX="79.0" layoutY="503.0" prefHeight="25.0" prefWidth="169.0" text="Choose Your Country" textFill="#807474">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </Label>
        <Button fx:id="register" layoutX="89.0" layoutY="549.0" mnemonicParsing="false" onAction="#confirmRegister" prefHeight="42.0" prefWidth="342.0" style="-fx-background-color: #486380;" text="Register" textFill="#f5f3f3">
